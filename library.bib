@article{2011MNRAS.411.2383K,
author = {Kwan, John and Fischer, William},
title = {{Origins of the H, He I and Ca II line emission in classical T Tauri stars}},
journal = {Monthly Notices of the Royal Astronomical Society},
year = {2011},
volume = {411},
number = {4},
pages = {2383--2425},
