journal = {The Astrophysical Journal},
year = {2008},
volume = {689},
number = {1},
pages = {316--334},
month = dec,
publisher = {IOP Publishing},
affiliation = {Harvard-Smithsonian Center for Astrophysics, 60 Garden Street, Cambridge, MA 02138},
doi = {10.1086/592566},
language = {English},
rating = {0},
date-added = {2019-01-16T11:38:31GMT},
date-modified = {2019-05-28T12:55:30GMT},
abstract = {Pre-main-sequence stars are observed to be surrounded by both accretion flows and some kind of wind or jetlike outflow. Recent work by Matt and Pudritz has suggested that if classical T Tauri stars exhibit stellar winds with mass-loss rates about 0.1 times their accretion rates, the wind can carry away enough angular momentum to keep the stars from being spun up unrealistically by accretion. This paper presents a preliminary set of theoretical models of accretion-driven winds from the polar regions of T Tauri stars. These models are based on recently published self-consistent simulations of the Sun's coronal heating and wind acceleration. In addition to the convection-driven MHD turbulence (which dominates in the solar case), we add another source of wave energy at the photosphere that is driven by the impact of plasma in neighboring flux tubes undergoing magnetospheric accretion. This added energy, determined quantitatively from the far-field theory of MHD wave generation, is sufficient to produce T Tauri-like mass-loss rates of at least 0.01 times the accretion rate. While still about an order of magnitude below the level required for efficient angular momentum removal, these are the first self-consistent models of T Tauri winds that agree reasonably well with a range of observational mass-loss constraints. The youngest modeled stellar winds are supported by Alfv{\'e}n wave pressure, they have low temperatures ("extended chromospheres"), and they are likely to be unstable to the formation of counterpropagating shocks and clumps far from the star.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2008ApJ...689..316C&link_type=EJOURNAL},
local-url = {file://localhost/Users/twilson/Dropbox/Library.papers3/Files/D7/D7712585-C53C-425E-BB06-C0BC6E5E309E.pdf},
file = {{D7712585-C53C-425E-BB06-C0BC6E5E309E.pdf:/Users/twilson/Dropbox/Library.papers3/Files/D7/D7712585-C53C-425E-BB06-C0BC6E5E309E.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1086/592566}}
}

