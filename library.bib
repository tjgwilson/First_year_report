@article{2019arXiv190508516A,
author = {Amard, L and Palacios, A and Charbonnel, C and Gallet, F and Georgy, C and Lagarde, N and Siess, L},
title = {{First grids of low-mass stellar models and isochrones with self-consistent treatment of rotation : From 0.2 to 1.5 M_\odot at 7 metallicities from PMS to TAMS}},
journal = {arXiv.org},
year = {2019},
eprint = {1905.08516},
