publisher = {IOP Publishing},
affiliation = {Harvard-Smithsonian Center for Astrophysics, 60 Garden Street, Cambridge, MA 02138; ncalvet@cfa.harvard.edu, lhartmann@ cfa.harvard.edu, egullbring@cfa.harvard.edu.},
doi = {10.1086/305277},
language = {English},
rating = {0},
date-added = {2019-05-30T11:00:21GMT},
date-modified = {2019-05-30T11:03:00GMT},
abstract = {Using results and calibrations from a previous paper (Gullbring et al. 1997), we estimate disk accretion rates for pre-main-sequence stars in the Taurus and Chamaeleon I molecular cloud complexes. The median accretion rate for T Tauri stars of age {\textasciitilde}1 Myr is {\textasciitilde}10-8 MâŠ™ yr-1 the intrinsic scatter at a given age may be as large as 1 order of magnitude. There is a clear decline of mass accretion rates {\.{M}} with increasing age t among T Tauri stars. Representing this decline as {\.{M}}{\textasciitilde}t-$\eta$, we estimate 1.5 <{\textasciitilde} $\eta$ <{\textasciitilde} 2.8; the large uncertainty is due to the wide range of accretion rates at a given age, the limited age range of the sample, and errors in estimating stellar ages and accretion luminosities. Adopting values of $\eta$ near the low end of this range, which are more likely given probable errors and the neglect of birthline age corrections, masses accreted during the T Tauri phase are roughly consistent with disk masses estimated from millimeter-wave dust emission. Similarity solutions for evolving, expanding disks are used to investigate observational constraints on disk properties employing a minimum of parameters. For an assumed power-law form of the disk viscosity with radius $\nu$ {\textasciitilde} R$\gamma$, $\eta$ >{\textasciitilde} 1.5 corresponds to $\gamma$ >{\textasciitilde} 1. The limit $\gamma$ {\textasciitilde} 1 corresponds to a roughly constant ``$\alpha$'' in the Shakura-Sunyaev (1973) viscosity parameterization; using current observed disk sizes, we estimate $\alpha$ {\textasciitilde} 10-2 (on scales {\textasciitilde}10-100 AU). Much of the observed variation in mass accretion rates can be accounted for by varying initial disk masses between 0.01 and 0.2 MâŠ™, but this result may be strongly affected by the presence of binary companion stars. These results emphasize the need for older samples of stars for studying disk evolution.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=1998ApJ...495..385H&link_type=EJOURNAL},
local-url = {file://localhost/Users/twilson/Dropbox/Library.papers3/Files/6F/6FF9F75F-06FE-4D7B-8635-4E15606FB782.pdf},
file = {{6FF9F75F-06FE-4D7B-8635-4E15606FB782.pdf:/Users/twilson/Dropbox/Library.papers3/Files/6F/6FF9F75F-06FE-4D7B-8635-4E15606FB782.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1086/305277}}
}

